                                                </td>
                                                <td class="product-name">
                                                    <h5><a href="product-details.html">{{this.product.name}}</a></h5>
                                                </td>
                                                <td class="product-cart-price"><span
                                                        class="amount">₹{{this.product.price}}</span>
                                                    {{!-- {{/each}} --}}

